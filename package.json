{
    "name": "shorty",
    "version": "1.0.0",
    "private": true,
    "description": "Convert long URI into a short one.",
    "keywords": [
        "Shorty",
        "URL",
        "URI"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/unbelt/shorty.git"
    },
    "author": "unbelt",
    "scripts": {
        "build-prod:watch": "webpack --env=prod --watch --color",
        "build:dev": "webpack --env=dev --progress",
        "build:prod": "webpack --env=prod",
        "build:watch": "webpack --env=dev --watch --color --progress",
        "deploy": "yarn docker:clean & yarn docker:build & yarn heroku:push & yarn heroku:release",
        "docker:build": "docker build -t unbelt/shorty .",
        "docker:clean": "docker system prune",
        "docker:run": "docker run -p 80:80 unbelt/shorty",
        "format:fix": "pretty-quick --staged",
        "heroku:push": "heroku container:login & heroku container:push web -a shorty-page",
        "heroku:release": "heroku container:release web -a shorty-page",
        "test": "karma start",
        "test:ci": "karma start --single-run --browsers ChromeHeadless",
        "update": "npx sort-package-json && yarn"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format:fix"
        }
    },
    "dependencies": {
        "@angular/animations": "9.1.4",
        "@angular/cdk": "9.2.2",
