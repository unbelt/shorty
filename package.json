{
    "name": "shorty",
    "version": "2.0.0",
    "private": true,
    "description": "Convert long URI into a short one.",
    "keywords": [
        "Shorty",
        "URL",
        "URI"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/unbelt/shorty.git"
    },
    "author": "unbelt",
    "scripts": {
        "build-prod:watch": "webpack --env=production --watch --color",
        "build:dev": "webpack --env=development --progress",
        "build:prod": "webpack --env=production",
        "build:watch": "webpack --env=development --watch --color --progress",
        "docker:build": "docker build -t unbelt/shorty .",
        "docker:clean": "docker system prune",
        "docker:clean:volume": "docker volume rm $(docker volume ls -q)",
        "docker:compose": "docker-compose up -d",
        "docker:run": "docker run -p 80:80 unbelt/shorty",
        "docker:stop": "docker-compose down",
        "format:fix": "pretty-quick --staged",
        "heroku:deploy": "yarn docker:clean & yarn docker:build & yarn heroku:push & yarn heroku:release",
        "heroku:push": "heroku container:login & heroku container:push web -a shorty-page",
        "heroku:release": "heroku container:release web -a shorty-page",
        "test": "karma start",
        "test:ci": "karma start --single-run --browsers ChromeHeadless",
        "update": "npx sort-package-json && yarn"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format:fix"
        }
    },
    "dependencies": {
        "@angular/animations": "11.2.12",
        "@angular/cdk": "11.2.11",
        "@angular/common": "11.2.12",
        "@angular/compiler": "11.2.12",
        "@angular/core": "11.2.12",
        "@angular/forms": "11.2.12",
        "@angular/material": "11.2.11",
        "@angular/platform-browser": "11.2.12",
        "@angular/platform-browser-dynamic": "11.2.12",
        "@angular/router": "11.2.12",
        "@ngrx/core": "1.2.0",
        "@ngrx/effects": "11.1.1",
        "@ngrx/store": "11.1.1",
        "@ngrx/store-devtools": "11.1.1",
        "core-js": "2.6.11",
        "lodash-es": "4.17.21",
        "process": "0.11.10",
        "rxjs": "6.6.0",
        "zone.js": "0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1102.11",
        "@angular-devkit/build-optimizer": "0.1102.11",
        "@angular-eslint/builder": "4.2.0",
        "@angular-eslint/eslint-plugin": "4.2.0",
        "@angular-eslint/eslint-plugin-template": "4.2.0",
        "@angular-eslint/schematics": "4.2.0",
        "@angular-eslint/template-parser": "4.2.0",
        "@angular/cli": "11.2.11",
        "@angular/compiler-cli": "11.2.12",
        "@ngtools/webpack": "11.2.11",
        "@types/jasmine": "3.6.0",
        "@types/lodash-es": "4.17.4",
        "@types/node": "12.11.1",
        "@typescript-eslint/eslint-plugin": "4.16.1",
        "@typescript-eslint/parser": "4.16.1",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "5.2.1",
        "circular-dependency-plugin": "5.2.2",
        "clean-webpack-plugin": "4.0.0-alpha.0",
        "copy-webpack-plugin": "6.4.0",
        "css-loader": "5.2.4",
        "eslint": "7.6.0",
        "eslint-webpack-plugin": "2.5.4",
        "html-webpack-plugin": "4.5.1",
        "husky": "6.0.0",
        "jasmine-core": "3.6.0",
