{
    "name": "shorty",
    "version": "2.0.0",
    "private": true,
    "description": "Convert long URI into a short one.",
    "keywords": [
        "Shorty",
        "URL",
        "URI"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/unbelt/shorty.git"
    },
    "author": "unbelt",
    "scripts": {
        "build-prod:watch": "webpack --env=production --watch --color",
        "build:dev": "webpack --env=development --progress",
        "build:prod": "webpack --env=production",
        "build:watch": "webpack --env=development --watch --color --progress",
        "docker:build": "docker build -t unbelt/shorty .",
        "docker:clean": "docker system prune",
        "docker:clean:volume": "docker volume rm",
        "docker:compose": "docker-compose up -d",
        "docker:run": "docker run -p 80:443 unbelt/shorty",
        "docker:stop": "docker-compose down",
        "format:all": "prettier --write **/*.{ts,scss,html}",
        "format:staged": "pretty-quick --staged",
        "test": "karma start",
        "test:ci": "karma start --single-run --browsers ChromeHeadless"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn format:fix"
        }
    },
    "dependencies": {
        "@angular/animations": "^15.2.0",
        "@angular/cdk": "^15.2.0",
        "@angular/common": "^15.2.0",
        "@angular/compiler": "^15.2.0",
        "@angular/core": "^15.2.0",
        "@angular/forms": "^15.2.0",
        "@angular/material": "^15.2.0",
        "@angular/platform-browser": "^15.2.0",
        "@angular/platform-browser-dynamic": "^15.2.0",
        "@angular/router": "^15.2.0",
        "@ngrx/effects": "^15.3.0",
        "@ngrx/store": "^15.3.0",
        "@ngrx/store-devtools": "^15.3.0",
        "core-js": "3.28.0",
        "lodash-es": "4.17.21",
        "process": "0.11.10",
        "rxjs": "7.8.0",
        "zone.js": "0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "15.2.0",
        "@angular-eslint/builder": "15.2.1",
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular-eslint/eslint-plugin-template": "15.2.1",
        "@angular-eslint/template-parser": "15.2.1",
        "@angular-eslint/schematics": "15.2.1",
        "@angular/cli": "~15.2.0",
        "@angular/compiler-cli": "^15.2.0",
        "@ngtools/webpack": "15.2.0",
        "@types/jasmine": "~4.3.0",
        "@types/lodash-es": "4.17.6",
        "@types/node": "18.14.1",
        "@typescript-eslint/eslint-plugin": "5.53.0",
        "@typescript-eslint/parser": "5.53.0",
        "angular2-template-loader": "0.6.2",
        "ts-loader": "9.4.2",
        "circular-dependency-plugin": "5.2.2",
        "clean-webpack-plugin": "4.0.0",
        "copy-webpack-plugin": "6.4.0",
        "css-loader": "6.7.3",
        "eslint": "8.34.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-webpack-plugin": "4.0.0",
        "html-webpack-plugin": "5.5.0",
        "husky": "8.0.3",
        "jasmine-core": "~4.5.0",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "karma-sourcemap-loader": "0.4.0",
        "karma-spec-reporter": "0.0.36",
        "karma-webpack": "5.0.0",
        "path-browserify": "1.0.1",
        "prettier": "2.8.4",
        "pretty-quick": "3.1.3",
        "raw-loader": "4.0.2",
        "sass-loader": "10.2.0",
        "source-map-loader": "1.1.3",
        "style-loader": "3.3.1",
        "ts-node": "10.9.1",
        "typescript": "4.9.5",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "5.75.0",
        "webpack-bundle-analyzer": "4.8.0",
        "webpack-cli": "5.0.1"
    }
}
