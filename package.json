        "@ngrx/core": "1.2.0",
        "@ngrx/effects": "9.1.0",
        "@ngrx/store": "9.1.0",
        "@ngrx/store-devtools": "9.1.0",
        "aspnet-prerendering": "3.0.1",
        "core-js": "2.6.11",
        "jquery": "3.5.0",
        "lodash-es": "4.17.15",
        "rxjs": "6.5.5",
        "zone.js": "0.10.3"
    },
